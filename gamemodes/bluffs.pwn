			JobStep[playerid] = 0;
			GivePlayerCash(playerid, -200);
			SS(playerid, COLOR_JOB, "(Job){FFFFFF} Ai platit $200 companiei de autobuze pentru taxe.","(-) You paid $200 to bus company for taxes.");
