	}
    SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
	tazer[playerid] = 0;
    if(OnDuty[playerid] > 0)
	{
		if(PlayerInfo[playerid][pGunLic] == 0 && IsACop(playerid))
