					if(PlayerInfo[caller][pPet] != 0 && PlayerInfo[caller][pPetStatus] != 0) AttachPet(caller);
					Mobile[playerid] = -1;
					CellTime[playerid] = 0;
					PlayerInfo[playerid][pSpeaker] = 0;
					SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
